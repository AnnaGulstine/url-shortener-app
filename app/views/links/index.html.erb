<div class="container">

<h1>Your links</h1>
  <table class="table table-striped table-hover">
    <th>Slug</th>
    <th>Target URL</th>
    <th>Created At</th>
    <% @links.each do |link| %>
      <tr>
        <td><%= link.slug %></td>
        <td><%= link.target_url %></td>
        <td><%= link.created_at %></td>
      </tr>
    <% end %>
  </table>

</div>